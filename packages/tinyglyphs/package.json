{
  "name": "tinyglyphs",
  "version": "0.1.1-alpha",
  "type": "module",
  "description": "A tiny collection of simple lined icons",
  "exports": {
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.mjs",
      "require": "./dist/react.js"
    },
    "./react/*": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.mjs",
      "require": "./dist/react.js"
    },
    "./vue": {
      "types": "./dist/vue.d.ts",
      "import": "./dist/vue.mjs",
      "require": "./dist/vue.js"
    },
    "./vue/*": {
      "types": "./dist/vue.d.ts",
      "import": "./dist/vue.mjs",
      "require": "./dist/vue.js"
    },
    "./svelte": {
      "types": "./dist/svelte.d.ts",
      "import": "./dist/svelte.mjs",
      "require": "./dist/svelte.js"
    },
    "./svelte/*": {
      "types": "./dist/svelte.d.ts",
      "import": "./dist/svelte.mjs",
      "require": "./dist/svelte.js"
    },
    "./angular": {
      "types": "./dist/angular.d.ts",
      "import": "./dist/angular.mjs",
      "require": "./dist/angular.js"
    },
    "./angular/*": {
      "types": "./dist/angular.d.ts",
      "import": "./dist/angular.mjs",
      "require": "./dist/angular.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "generate": "node scripts/generate.cjs",
    "test": "jest --config jest.config.cjs",
    "test:coverage": "jest --config jest.config.cjs --coverage",
    "test:bundle": "jest --config jest.config.cjs tests/build-process.test.js",
    "analyze-bundle": "node scripts/analyze-bundle.cjs",
    "prepublishOnly": "pnpm clean && pnpm generate && pnpm build && pnpm test"
  },
  "keywords": [
    "tinyglyphs",
    "icons",
    "svg",
    "react",
    "vue",
    "svelte",
    "angular",
    "typescript",
    "components",
    "ui",
    "icon-library"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/madebyankur/tinyglyphs.git"
  },
  "homepage": "https://tinyglyphs.ankur.design",
  "bugs": {
    "url": "https://github.com/madebyankur/tinyglyphs/issues"
  },
  "author": "Ankur Chauhan <hi@ankurchauhan.design>",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "jest": "^29.5.0",
    "jest-environment-node": "^29.5.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "vue": ">=3.0.0",
    "svelte": ">=3.0.0",
    "@angular/core": ">=12.0.0",
    "@angular/common": ">=12.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "svelte": {
      "optional": true
    },
    "@angular/core": {
      "optional": true
    },
    "@angular/common": {
      "optional": true
    }
  },
  "dependencies": {
    "clsx": "^2.0.0"
  },
  "browserslist": [
    "defaults",
    "not ie 11"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
}
